  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.3.15",
    "babel-core": "^6.2.0",
    "babel-plugin-transform-object-rest-spread": "^6.3.13",
    "babel-preset-es2015": "^6.1.18",
    "mocha": "^2.3.4",
    "rimraf": "^2.4.4",
    "snazzy": "^2.0.1",
